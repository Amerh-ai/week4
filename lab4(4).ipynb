   "source": [
    "n = int(input(\"Enter n: \"))\n",
    "count = 0\n",
    "for i in range(1, n + 1):\n",
    "    if i > 50:\n",
    "        break\n",
    "\n",
    "    if i % 8 == 0:\n",
    "        continue\n",
    "\n",
    "    if i % 4 == 0:\n",
    "        count += 1\n",
    "\n",
    "print(\"Count =\", count)"
   ]
